<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="wims">
        <item name="android:textSize">48sp</item>
        <item name="android:letterSpacing">-0.01</item>
        <item name="android:textColor">#0038A8</item>
    </style>

    <style name="main_menu">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#181818</item>
    </style>

    <style name="login_title">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#F6B601</item>
        <item name="android:shadowColor">#26000000</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">2</item>
        <item name="android:shadowRadius">2</item>
    </style>

    <style name="login_title1">
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">#0038A8</item>
        <item name="android:shadowColor">#26000000</item>
        <item name="android:shadowDx">0</item>
        <item name="android:shadowDy">2</item>
        <item name="android:shadowRadius">2</item>
    </style>

    <style name="swipe_down_">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#BDBDBD</item>
    </style>

    <style name="FormField" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColorHint">@color/placeholder</item>
    </style>



</resources>